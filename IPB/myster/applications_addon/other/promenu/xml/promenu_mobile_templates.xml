    <template>
      <template_group>skin_promenu_plus</template_group>
      <template_content><![CDATA[<div id="{$id}_js">
	<div class="{$id}">
		<div class="{$id}-title">Menu</div>
			<div class="{$id}-button" id="responsiveMenu{$id}">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</div>
		</div>
		<div id="{$id}">
			<div class="main_widths">
				<ul <if test="$group['promenu_groups_default_position'] == 2">class="fixed"</if> id="{$id}_menu">
					{$menus}
				</ul>
			</div>
		</div>
	</div>
<script>
(function($) {
	$(document).ready(function(){
		var show = "{$group['promenu_groups_animation_open']}",
		hidden = "{$group['promenu_groups_animation_close']}";
		
		$("#responsiveMenu{$id}").click(function(){
			if(!$("#{$id}_menu").data("isOpen")){
				$(this).addClass("open");
				if(show == "show"){
					$("#{$id}_menu").data("isOpen",1).show({$group['promenu_groups_speed_open']});
				}
				else if(show == "fade"){
					$("#{$id}_menu").data("isOpen",1).fadeIn({$group['promenu_groups_speed_open']});
				}
				else if(show == "slide"){
					$("#{$id}_menu").data("isOpen",1).slideDown({$group['promenu_groups_speed_open']});
				}
			}
			else{
				$(this).removeClass("open");
				if(hidden == "hide"){
					$("#{$id}_menu").removeData("isOpen").hide({$group['promenu_groups_speed_open']});
				}
				else if(hidden == "fade"){
					$("#{$id}_menu").removeData("isOpen").fadeOut({$group['promenu_groups_speed_open']});
				}
				else if(hidden == "slide"){
					$("#{$id}_menu").removeData("isOpen").slideUp({$group['promenu_groups_speed_open']});
				}
				$("#{$id}").find(".{$id}-button2").each(function() {
					if($(this).hasClass('open')){
						$(this).removeClass('open').prev().removeData("isOpen").hide();
					}
				});
			}
		});

		$("#{$id}_menu").ProMenuJsR({	show: "{$group['promenu_groups_animation_open']}",
										hidden: "{$group['promenu_groups_animation_close']}",
										id: "{$id}",
										openSpeed:{$group['promenu_groups_speed_open']},
										closeSpeed:{$group['promenu_groups_speed_close']}});
		});	
	
}(projQ));
</script>]]></template_content>
