      <conf_type>input</conf_type>
      <conf_key>promenu_jquery_ui_css</conf_key>
      <conf_default>http://code.jquery.com/ui/1.9.2/themes/redmond/jquery-ui.css</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>1</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title>jQuery Remote Load</conf_title>
      <conf_description>load jQuery remotely. (will also load jQuery UI remotely, where it is used).</conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>promenu_menus_remote_jquery</conf_key>
      <conf_default>1</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
