				</if>
				<if test="$data['promenu_menus_has_sub']">
					<foreach loop="$menu[$data['promenu_menus_id']] as $k => $c">
						<div class="othersubs">
							{parse template="proOther" group="promenu_plus" params="$c, $menu, $group, 0"}
						</div>
					</foreach>
				</if>
			</if>
		</div>]]></template_content>
