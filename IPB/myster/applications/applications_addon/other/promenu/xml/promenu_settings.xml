<?xml version="1.0" encoding="utf-8"?>
<settingexport>
  <settinggroup>
    <setting>
      <conf_is_title>1</conf_is_title>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_title_title>Global Settings</conf_title_title>
      <conf_title_desc/>
      <conf_title_tab>ProMenu</conf_title_tab>
      <conf_title_app>promenu</conf_title_app>
      <conf_title_noshow>1</conf_title_noshow>
    </setting>
    <setting>
      <conf_title>jQuery UI CSS</conf_title>
      <conf_description>Link to use for jQuery UI css.(can get different css from jquery ui cdn, http://jqueryui.com/download/ or you can leave this blank, and load your jquery UI css thru IPB template system.)</conf_description>
      <conf_type>input</conf_type>
      <conf_key>promenu_jquery_ui_css</conf_key>
      <conf_default>http://code.jquery.com/ui/1.9.2/themes/redmond/jquery-ui.css</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>1</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title>jQuery Remote Load</conf_title>
      <conf_description>load jQuery remotely. (will also load jQuery UI remotely, where it is used).</conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>promenu_menus_remote_jquery</conf_key>
      <conf_default>1</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>2</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title><![CDATA[Disable &quot;More&quot;]]></conf_title>
      <conf_description><![CDATA[disabling this will disable the default "more" where it is used.]]></conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>promenu_disable_more</conf_key>
      <conf_default>0</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>3</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title><![CDATA[Disable News &amp; Version Check]]></conf_title>
      <conf_description><![CDATA[if you don't want the "menus" section to display news or check for new versions, select yes here. this is good if you server has crappy dns lookup servers, or for whatever reason provisionists.com might be down.]]></conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>promenu_checks_api</conf_key>
      <conf_default>0</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>4</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title>Enable Content Block Injection</conf_title>
      <conf_description>Enabling the content block injection, will give access to the default css and js for content blocks. </conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>promenu_disable_content_css</conf_key>
      <conf_default>0</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>5</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title><![CDATA[Disable &quot;tablet&quot; pop up]]></conf_title>
      <conf_description>Disable the pop up that appears for touch devices, this will use whatever "behavoir" setting you have listed in the group settings.</conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>promenu_disable_tab_pop</conf_key>
      <conf_default>0</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>6</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>promenu_menus_settings</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_is_title>1</conf_is_title>
      <conf_title_keyword>promenu_menus_settings_hidden</conf_title_keyword>
      <conf_title_title><![CDATA[Hidden for good reason&#33;]]></conf_title_title>
      <conf_title_desc/>
      <conf_title_tab>ProMenu</conf_title_tab>
      <conf_title_app>promenu</conf_title_app>
      <conf_title_noshow>1</conf_title_noshow>
    </setting>
  </settinggroup>
</settingexport>
