<?xml version="1.0" encoding="utf-8"?>
<settingexport>
  <settinggroup>
    <setting>
      <conf_is_title>1</conf_is_title>
      <conf_title_keyword>bbcodeTables</conf_title_keyword>
      <conf_title_title>Custom BBCode Tables</conf_title_title>
      <conf_title_desc>The use of custom tables in bbcode</conf_title_desc>
      <conf_title_tab>IPBAccess</conf_title_tab>
      <conf_title_app>bbcodeTables</conf_title_app>
      <conf_title_noshow>0</conf_title_noshow>
    </setting>
    <setting>
      <conf_title>Custom BBCode Tables On</conf_title>
      <conf_description>Custom BBCode Tables on or off</conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>bbcodeTables_on</conf_key>
      <conf_start_group>Custom BBCode Tables - Main Settings</conf_start_group>
      <conf_default>0</conf_default>
      <conf_protected>1</conf_protected>
      <conf_position>10</conf_position>
      <conf_add_cache>1</conf_add_cache>
      <conf_title_keyword>bbcodeTables</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>

    <setting>
      <conf_title>Groups access</conf_title>
      <conf_description/>
      <conf_type>multi</conf_type>
      <conf_key>bbcodeTables_groups</conf_key>
      <conf_default>4</conf_default>
      <conf_extra>#show_groups#</conf_extra>
      <conf_evalphp><![CDATA[$key = 'bbcodeTables_groups';
if ( $save == 1) {
    if ( is_array($_POST[ $key ]) ) {
        $_POST[ $key ] = implode(",",$_POST[ $key ]);
    } else {
        $_POST[ $key ] = "";
    }
}
if ( $show == 1 ) {
    $key .= '[]';
}]]></conf_evalphp>
      <conf_protected>1</conf_protected>
      <conf_position>20</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>bbcodeTables</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>

    <setting>
      <conf_title>Forums</conf_title>
      <conf_description>Forums that will be effective</conf_description>
      <conf_type>multi</conf_type>
      <conf_key>bbcodeTables_forums</conf_key>
      <conf_extra>#show_forums#</conf_extra>
      <conf_evalphp><![CDATA[$key = 'bbcodeTables_forums';
if ( $save == 1){
    if ( is_array($_POST[ $key ]) )    {
        $_POST[ $key ] = implode(",",$_POST[ $key ]);
    } else {
        $_POST[ $key ] = "";
    }
}
if ( $show == 1 ) {
    $key .= '[]';
}]]></conf_evalphp>
      <conf_protected>1</conf_protected>
      <conf_position>30</conf_position>
      <conf_add_cache>1</conf_add_cache>
      <conf_title_keyword>bbcodeTables</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>

    <setting>
      <conf_title>Custom Teplate</conf_title>
      <conf_description>Custom Template</conf_description>
      <conf_type>yes_no</conf_type>
      <conf_key>bbcodeTables_usect</conf_key>
      <conf_start_group></conf_start_group>
      <conf_default>0</conf_default>
      <conf_protected>1</conf_protected>
      <conf_position>35</conf_position>
      <conf_add_cache>1</conf_add_cache>
      <conf_title_keyword>bbcodeTables</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
