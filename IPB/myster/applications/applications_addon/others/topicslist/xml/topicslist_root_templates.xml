    <template>
      <template_group>skin_list</template_group>
      <template_content><![CDATA[<div class='category_block block_wrap' />
	<table class='ipb_table topic_list' "{$this->lang->words['tl_choosef']}"' id='forum_table' style="width: 75%; margin: auto;">
				<caption class='maintitle'>
					<span class='main_forum_title'>{$this->lang->words['tl_choosef']}</span>
				</caption>
				<tr class='header'>					
					<th scope='col' style='width: 40%;' >{$this->lang->words['tl_lforum']}</th>
					<if test="isdesc:|:$desc == 1"><th scope='col' style='width: 40%;' >{$this->lang->words['tl_fdesc']}</th></if>
					<th scope='col' style='width:30%;  text-align: center;' >{$this->lang->words['tl_ntopics']}</th>
				</tr>	
 
				<!-- Lets Go With The Cycle! -->
				<foreach loop="showlist:$forums as $fid">
				
				<if test="iscat:|:$fid['iscat'] == 1">
					<tr class='header'>	
							<td >{$fid['idcat']}{$fid['name']}</td>
							<if test="descdesc:|:$desc == 1"><td ><span>{$fid['desc']}</span></td></if>
							<td ></td>
							
					</tr>
				<else />
					<tr>		
							<td class='row2'  ><a href="{parse url="app=topicslist&amp;module=list&amp;section=view&amp;f={$fid['id']}" base="public" "}" class='topic_title'>{$fid['name']}</a></td>
							<if test="descdesc:|:$desc == 1"><td class='row2' ><span>{$fid['desc']}</span></td></if>
							<td class='row2' style='text-align: center;' ><span >&nbsp;{$fid['ntopics']}</span></td>
							
					</tr>
				</if>
				
		</foreach>
	</table>
</div>
<p style='text-align: right; font-size: 9px;">Powered by Topics Wall 1.6. Developed and Designed by Devian</p>]]></template_content>
