    <template>
      <template_group>skin_list</template_group>
      <template_content><![CDATA[<table class="ipb_table" cellspacing="1">
	<tr class='header'>
		<th ></th>
		<th></th>
	</tr>
		
		<foreach loop="$topics as $topic">
			<tr>
				<td class="row2" style="vertical-align: top; width: 60px;">
					<a href="{parse url="showuser={$topic['mid']}" template="showuser" seotitle="{$topic['mid_seo']}" base="public"}">{$topic['avatar']}</a>
				</td>
				
				<td class="row2">
					<div class="insideinfos">
						<h6 class="fbttitle">
							<img src="{$this->settings['img_url']}/user.png" alt="" /> 
								<a href="{parse url="showuser={$topic['mid']}" template="showuser" seotitle="{$topic['mid_seo']}" base="public"}">{$topic['starter']}</a>
						</h6>
						
						<div style='display:block;'>
							<div>
								<table>
									<tr>
										<td  class="fbtdprev" style="border-bottom: none;">								
											<a href='{parse url="showtopic={$topic['tid']}" template="showtopic" seotitle="{$topic['tseo']}" base="public"}' title='{$this->lang->words['tl_gotot']}'>
												<p style="text-align: center;"><img src='{$topic['cover']}' class="fb_previmg" ></p>
											</a>
										</td>
										<td style="border-bottom: none; vertical-align: top; padding: 5px;">
											<div class="titleinfo" >
												<p>
													<img src="{$this->settings['img_url']}/bullet.png" alt="" />
													<a href='{parse url="showtopic={$topic['tid']}" template="showtopic" seotitle="{$topic['tseo']}" base="public"}' title='{$this->lang->words['tl_gotot']}'>{$topic['title']}</a>
													<if test="$this->settings['tl_tracker'] && $topic['watched']">
														<img src="{$this->settings['img_url']}/transmit_blue.png">
													</if>
												</p>
												<p>{$topic['description']}</p><br>
												<if test="$topic['post'] != ''">
													<p style="width: 300px;">{$topic['post']}...</p>
												</if>
												
												<if test="$this->request['f'] == ''">
													<p>
														<strong>{$this->lang->words['tl_onf']}:</strong> <a href='{parse url="showforum={$topic['fid']}" template="showforum" seotitle="{$topic['fseo']}" base="public"}' title=''>{$topic['forum']}</a>
													</p>
												</if>
											</div>
										</td>
									</tr>
								</table>
							</div>
						</div>							
							
							<span class="iconinfo">
								<img src="{$this->settings['img_url']}/time.png" alt="" />
								<if test="$this->request['sort'] == 'update'">
									{parse date="$topic['lpost']" format="SHORT"}
								<else />
									{parse date="$topic['date']" format="SHORT"}
								</if>
							</span>
							<span class="iconinfo"> 
								<img src="{$this->settings['img_url']}/comments.png" alt="" /> {$topic['tposts']} {$this->lang->words['tl_comm']} 
							</span>
							<span class="iconinfo"> 
								<img src="{$this->settings['img_url']}/visits.png" alt="" />  {$topic['tviews']} {$this->lang->words['tl_viewed']} 
							</span>				
							<span class='mini_rate' >
								<foreach loop="ratings:array(1,2,3,4,5) as $int">
									<if test="ratinggood:|:$topic['rate'] >= $int">
										{parse replacement="mini_rate_on"}
									<else />
										{parse replacement="mini_rate_off"}
									</if>
								</foreach>
							</span>
						</div>
					</div>
					{$topic['test']}
				</td>				
			</tr>
			
		</foreach>
		
</table>
</div>
</div>
