    <template>
      <template_group>skin_list</template_group>
      <template_content><![CDATA[<table >
		<tr class='header'>
			<th class="detimg" ></th>
			<th style='width: 35%;'></th>
			<th class="detimg" ></th>
			<th style='width: 35%;'></th>
		</tr>
		
		<foreach loop="home-list:$topics as $tid">
		
			<if test="isnothing:|:$tid['title'] == ''">
			
					<td class="row2 detprevimg" ></td>
					<td class="row2" ></td>
				
			<else />
			
				<if test="isnew:|: $tid['number'] == 1">
					<tr>
				</if>
				
				<td class="row2 detprevimg"  >
					<a href='{parse url="showtopic={$tid['tid']}" template="showtopic" seotitle="{$tid['tseo']}" base="public"}' title='{$this->lang->words['tl_gotot']}'><img src="{$tid['cover']}" class="det_img" ></a>
				</td>
				<td class="row2" >
					<div class="dettinfos" >
						<ul>
							<li class="infoelement">
								<a href='{parse url="showtopic={$tid['tid']}" template="showtopic" seotitle="{$tid['tseo']}" base="public"}' title='{$this->lang->words['tl_gotot']}'>{$tid['title']} </a> 
								
									<if test="$this->settings['tl_tracker'] && $tid['watched']">
										<img src="{$this->settings['img_url']}/transmit_blue.png">
									</if>
									
								<span class='mini_rate' >
									<foreach loop="ratings:array(1,2,3,4,5) as $int">
										<if test="ratinggood:|:$tid['rate'] >= $int">
											{parse replacement="mini_rate_on"}
										<else />
											{parse replacement="mini_rate_off"}
										</if>
									</foreach>
								</span>
							</li>
							
							<li class="infoelement">{$tid['description']} 
								<if test="iscomments:|:$this->request['sort'] == 'comments'">{$tid['tposts']} {$this->lang->words['tl_comm']}</if>
								<if test="isvisits:|:$this->request['sort'] == 'visits'"> {$tid['tviews']} {$this->lang->words['tl_viewed']}</if>
							</li>			
							
							<li class="infoelement">
								<if test="$this->request['sort'] == 'update'">{parse date="$tid['lpost']" format="SHORT"}<else />{parse date="$tid['date']" format="SHORT"} </if>
								<if test="f:|:$this->request['f'] ==''">
									<span class="infoelement">{$this->lang->words['tl_onf']}</span> 
									<a href='{parse url="showforum={$tid['fid']}" template="showforum" seotitle="{$tid['fseo']}" base="public"}' title=''>{$tid['forum']}</a>
								</if>
							</li>
							
							<li class="infoelement">{$this->lang->words['tl_author']}: <a href="{parse url="showuser={$tid['mid']}" template="showuser" seotitle="{$tid['mid_seo']}" base="public"}">{$tid['starter']}</a>
							</li>
							<br/>
							
							<if test="isempty:|:$tid['post'] != ''">
								<li class="infoelement" >
									<div >
										<span>{$tid['post']}...</span>
									</div>
								</li>
							</if>
							
						</ul>
					</div>
				</td>
				
				<if test="isend:|:$tid['number'] == 2">
					</tr>
				</if>
				
			</if>
			
		</foreach>
	</table>
</div>
</tr>	
</table>
</div>
</div>
