  <templategroup group="skin_list">
    <template>
      <template_group>skin_list</template_group>
      <template_content><![CDATA[<table class="ipb_table" cellspacing="1">
	<tr class='header'>
		<th style='width: 30%;'>{$this->lang->words['tl_cltitle']}</th>
		<th class="authorth" align="center">{$this->lang->words['tl_clauthor']}</th>
			<if test="$this->request['f'] == ''">
				<th style='width: 20%;' align="left">{$this->lang->words['tl_clforum']}</th>
			</if>
		<if test="$this->request['l'] != 'all'">
			<th style='width: 20%;'>
				<if test="$temp_sort != 'alpha'">{$char}</if>
			</th>
		</if>
	</tr>
	
	<foreach loop="home-list:$topics as $tid">
		<tr>
			<td class="row2">
				<a href='{parse url="showtopic={$tid['tid']}" template="showtopic" seotitle="{$tid['tseo']}" base="public"}' title='go to topic#'>{$tid['title']}</a> 
					<if test="$this->settings['tl_tracker'] && $tid['watched']">
						<img src="{$this->settings['img_url']}/transmit_blue.png">
					</if>
				
					<if test="isall:|:strtolower( $this->request['l'] ) != 'all'">
						<if test="$temp_sort != 'rate'">
							<span class='mini_rate' >
								<foreach loop="ratings:array(1,2,3,4,5) as $int">
									<if test="ratinggood:|:$tid['rate'] >= $int">
										{parse replacement="mini_rate_on"}
									<else />
										{parse replacement="mini_rate_off"}
									</if>
								</foreach>
							</span>
						</if>
					</if>
					<br>
					
				<div style='margin-left: 15px;'>
					<span class='description' >{$tid['description']}</span>
				</div>
			</td>
	
			<td class="row2" >
				<a href="{parse url="showuser={$tid['mid']}" template="showuser" seotitle="{$tid['mid_seo']}" base="public"}">{$tid['starter']}</a>
			</td>
			
				<if test="isalltopics:|:strtolower( $this->request['l'] ) != 'all'">
					<if test="$this->request['f'] == '' ">
						<td class="row2">
							<span style='font-weight: bold; '>
								<a href='{parse url="showforum={$tid['fid']}" template="showforum" seotitle="{$tid['fseo']}" base="public"}' title=''>{$tid['forum']}</a>
							</span>
						</td>
					</if>
					
					<if test="istheresort:|:$temp_sort != '' ">
						<td class="row2">
							<if test="$temp_sort == 'comments'">{$tid['tposts']} {$this->lang->words['tl_comm']}</if>
							<if test="$temp_sort == 'update'">{parse date="$tid['lpost']" format="SHORT"}</if>
							<if test="$temp_sort == 'rate'">
						
						<span class='mini_rate' >
							<foreach loop="ratings:array(1,2,3,4,5) as $int">
								<if test="ratinggood:|:$tid['rate'] >= $int">
									{parse replacement="mini_rate_on"}
								<else />
									{parse replacement="mini_rate_off"}
								</if>
							</foreach>
						</span>
					</if>
			
					<if test="$temp_sort == 'visits'">{$tid['tviews']} {$this->lang->words['tl_clvisits']}</if>
		
					<if test="$temp_sort == 'latests'">{parse date="$tid['date']" format="SHORT"} </if>
					
					</td>
					</if>
				</if>		
				
		</tr>
				
	</foreach>
</table>
</div>
</tr>	
</table>
</div>
	<p style="text-align: right; font-size: 9px; text-shadow: 0px 3px 5px #000;">
		<br />Powered by Topics Wall 1.6
		<br />Developed by devRegan. All rights reserved.
	</p>
	</div>]]></template_content>
